SIDESHIFT_SECRET=your_sideshift_secret_here
NEXT_PUBLIC_AFFILIATE_ID=your_affiliate_id_here
NEXT_PUBLIC_SIDESHIFT_API_URL=https://sideshift.ai/api/v2
SIDESHIFT_API_URL=https://sideshift.ai/api/v2
AFFILIATE_ID=your_affiliate_id_here

# Wallet Configuration
# Get from https://cloud.walletconnect.com/
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

